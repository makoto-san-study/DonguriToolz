{


  "manifest_version": 3,
  "name": "DonguriToolz",
  "version": "0.2.0",
  "description": "DonguriToolz",
  
  "permissions":[
    "storage"
  ],
  
  "options_ui": {"page": "options/options.html"},
  
  "background":{"service_worker": "background/background.js"},
  
  "content_scripts": [
    {"matches": ["https://donguri.5ch.net/bag"], "js": ["contents/bag.js"]},
    {"matches": ["https://donguri.5ch.net/dummy"], "js": ["contents/dummy.js"]}
  ]
}
